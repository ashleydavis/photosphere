{
  "name": "node-utils",
  "version": "1.0.0",
  "description": "Various utility functions that only work under Node.js.",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "t": "bun run test",
    "tw": "bun run test:watch",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "c": "bun run compile",
    "cw": "bun run compile:watch",
    "compile": "tsc",
    "compile:watch": "tsc --watch --preserveWatchOutput",
    "clean": "rm -rf build dist coverage tsconfig.tsbuildinfo"
  },
  "keywords": [],
  "author": "ashley@codecapers.com.au",
  "license": "MIT",
  "devDependencies": {
    "@peculiar/webcrypto": "^1.4.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.17.5",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^22.9.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "fs-extra": "^11.1.0",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "dayjs": "^1.11.7",
    "defs": "workspace:*",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "node-machine-id": "^1.1.12",
    "tools": "workspace:*",
    "utils": "workspace:*"
  }
}
