    "ts-jest": "^29.0.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "express": "^5.0.0-beta.1",
    "api": "workspace:*",
    "storage": "workspace:*",
    "utils": "workspace:*"
  }
