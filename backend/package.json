{
    "name": "photosphere-backend",
    "version": "1.0.0",
    "description": "",
    "main": "build/index.js",
    "scripts": {
        "start": "node build/index.js",
        "start:dev": "npm run clean-uploads && concurrently \"npm run start-db\" \"npm run start-with-dev-db\"",
        "start-db": "insta-mongo --db-port=7001 --rest-port=7000 --load=50-assets --fixtures=../fixtures --db=photosphere",
        "start-with-dev-db": "npm run clean-uploads && npm run copy-uploads && cross-env PORT=3000 DB_CONNECTION_STRING=mongodb://localhost:7001 ts-node-dev --respawn src/index.ts",
        "build": "npm install --production=false && npm run compile",
        "compile": "tsc",
        "compile:watch": "tsc --watch",
        "clean-uploads": "rm -rf uploads thumbs",
        "copy-uploads": "cp -r ../fixtures/50-assets/files/uploads uploads && cp -r ../fixtures/50-assets/files/thumbs thumbs",
        "clean": "rm -rf build",
        "test": "jest --coverage --runInBand",
        "test:watch": "jest --watch --runInBand"
    },
    "keywords": [],
    "author": "ashley@codecapers.com.au",
    "license": "MIT",
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.16",
        "@types/fs-extra": "^11.0.1",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.11.18",
        "@types/supertest": "^2.0.12",
        "concurrently": "^7.2.2",
        "cross-env": "^7.0.3",
        "fs-extra": "^11.1.0",
        "insta-mongo": "^0.0.6",
        "jest": "^29.4.1",
        "nodemon": "^2.0.16",
        "supertest": "^6.3.3",
        "ts-jest": "^29.0.5",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "aws-sdk": "^2.1313.0",
        "cors": "^2.8.5",
        "dayjs": "^1.11.7",
        "express": "^5.0.0-beta.1",
        "mongodb": "^4.7.0"
    }
}
