{
  "name": "upload",
  "description": "An upload tool for Photosphere.",
  "main": "src/index.js",
  "scripts": {
    "s": "pnpm run start",
    "sd": "pnpm run start:dev",
    "start": "ts-node src/index.ts",
    "start:dev": "ts-node-dev --respawn src/index.ts",
    "c": "pnpm run compile",
    "cw": "pnpm run compile:watch",
    "compile": "tsc",
    "compile:watch": "tsc --watch --preserveWatchOutput",
    "clean": "rm -rf build"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "dayjs": "^1.11.7",
    "exif-parser": "^0.1.12",
    "ffmpeg-ffprobe-static": "^6.1.1",
    "fluent-ffmpeg": "^2.1.3",
    "sharp": "^0.33.4",
    "user-interface": "workspace:*",
    "defs": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
