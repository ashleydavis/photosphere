{
  "name": "cli",
  "version": "0.0.8",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "t": "bun run test",
    "tw": "bun run test:watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "c": "bun run compile",
    "cw": "bun run compile:watch",
    "compile": "tsc",
    "compile:watch": "tsc --watch --preserveWatchOutput",
    "clean": "rm -rf build dist coverage tsconfig.tsbuildinfo bin",
    "start": "bun run index.ts",
    "start:debug": "bun --inspect run index.ts",
    "dev": "bun run --hot index.ts",
    "d": "bun run dev",
    "build-linux": "bun build --env=disable --compile --minify --sourcemap --target=bun-linux-x64-baseline --outfile bin/x64/linux/psi ./index.ts ./worker.ts",
    "build-win": "bun build --env=disable --compile --minify --sourcemap --target=bun-windows-x64 --outfile bin/x64/win/psi.exe ./index.ts ./worker.ts",
    "build-win-baseline": "bun build --env=disable --compile --minify --sourcemap --target=bun-windows-x64-baseline --outfile bin/x64/win/psi.exe ./index.ts ./worker.ts",
    "build-mac-x64": "bun build --env=disable --compile --minify --sourcemap --target=bun-darwin-x64 --outfile bin/x64/mac/psi ./index.ts ./worker.ts",
    "build-mac-arm64": "bun build --env=disable --compile --minify --sourcemap --target=bun-darwin-arm64 --outfile bin/arm64/mac/psi ./index.ts ./worker.ts",
    "test-simple-database": "rm -rf ./test/db && bun run start -- init ./test/db && bun run start -- add ./test/db ../../test/test.png"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.9",
    "cross-env": "^7.0.3",
    "jest": "^29.0.1",
    "ts-jest": "^29.0.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "api": "workspace:*",
    "commander": "^13.1.0",
    "exif-parser": "^0.1.12",
    "is-unicode-supported": "^2.1.0",
    "js-yaml": "^4.1.0",
    "merkle-tree": "workspace:*",
    "mime": "^4.0.7",
    "node-utils": "workspace:*",
    "open": "^10.1.2",
    "picocolors": "^1.1.1",
    "sisteransi": "^1.0.5",
    "storage": "workspace:*",
    "tools": "workspace:*",
    "utils": "workspace:*",
    "wrap-ansi": "^9.0.0",
    "bdb": "workspace:*",
    "defs": "workspace:*",
    "task-queue": "workspace:*",
    "debug-server": "workspace:*"
  }
}