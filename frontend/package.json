{
  "name": "photosphere-frontend",
  "version": "1.0.0",
  "description": "The frontend for the Photosphere application.",
  "source": "src/index.html",
  "scripts": {
    "start": "cross-env NODE_ENV=development BASE_URL=http://localhost:3000 GOOGLE_API_KEY=\"\" webpack serve --open",
    "start:prod": "cross-env NODE_ENV=production BASE_URL=https://photosphere-api.codecapers.com.au GOOGLE_API_KEY=\"\" webpack serve --open --mode production --devtool source-map",
    "start:no-open": "cross-env NODE_ENV=development BASE_URL=http://localhost:3000 GOOGLE_API_KEY=\"\" webpack serve",
    "build": "cross-env NODE_ENV=production BASE_URL=https://photosphere-api.codecapers.com.au webpack --mode production --devtool source-map",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "start-backend-for-e2e-tests": "cd ../backend && pnpm run start-for-e2e-tests",
    "start-frontend-for-e2e-tests": "wait-port 3000 && pnpm start:no-open",
    "start-for-e2e-tests": "concurrently --kill-others --names \"backend,frontend\" \"pnpm run start-backend-for-e2e-tests\" \"pnpm run start-frontend-for-e2e-tests\"",
    "test-e2e": "playwright test",
    "test-e2e:debug": "playwright test --debug",
    "compile": "tsc",
    "compile:watch": "tsc --watch",
    "clean": "rm -rf dist build"
  },
  "keywords": [],
  "author": "ashley@codecapers.com.au",
  "license": "MIT",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@playwright/test": "^1.30.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.18",
    "canvas": "^2.11.2",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.10.0",
    "fs-extra": "^11.1.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.4.1",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.5",
    "user-interface": "workspace:*",
    "wait-port": "^1.0.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@react-hook/resize-observer": "^1.2.6",
    "axios": "^0.27.2",
    "blob-util": "^2.0.2",
    "dayjs": "^1.11.7",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1"
  }
}
