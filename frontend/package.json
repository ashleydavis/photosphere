{
  "name": "photosphere-frontend",
  "version": "1.0.0",
  "description": "The frontend for the Photosphere application.",
  "source": "src/index.html",
  "scripts": {
    "start": "cross-env BASE_URL=http://localhost:3000 parcel",
    "build": "parcel build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "start-backend-for-e2e-tests": "cd ../backend && npm run start-for-e2e-tests",
    "start-frontend-for-e2e-tests": "wait-port 3000 && npm start",
    "start-for-e2e-tests": "concurrently --kill-others --names \"backend,frontend\" \"npm run start-backend-for-e2e-tests\" \"npm run start-frontend-for-e2e-tests\"",
    "test-e2e": "playwright test --reporter=list",
    "test-e2e:debug": "playwright test --reporter=list --debug",
    "compile": "tsc",
    "compile:watch": "tsc --watch",
    "clean": "rm -rf dist .parcel-cache build"
  },
  "keywords": [],
  "author": "ashley@codecapers.com.au",
  "license": "MIT",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@peculiar/webcrypto": "^1.4.1",
    "@playwright/test": "^1.30.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.4.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-infinite-scroller": "^1.2.3",
    "buffer": "^6.0.3",
    "canvas": "^2.11.2",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "events": "^3.3.0",
    "fs-extra": "^11.1.0",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.4.1",
    "parcel": "^2.7.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.20",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4",
    "wait-port": "^1.0.4"
  },
  "dependencies": {
    "@react-hook/resize-observer": "^1.2.6",
    "axios": "^0.27.2",
    "blob-util": "^2.0.2",
    "dayjs": "^1.11.7",
    "jszip": "^3.10.1",
    "mime-types": "^2.1.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroller": "^1.2.6",
    "react-router-dom": "^6.4.1"
  }
}
