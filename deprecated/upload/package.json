{
  "name": "upload",
  "description": "An upload tool for Photosphere.",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "s": "pnpm run start",
    "sd": "pnpm run start:dev",
    "start": "bun src/index.ts",
    "start:dev": "bun --hhot src/index.ts",
    "c": "pnpm run compile",
    "cw": "pnpm run compile:watch",
    "compile": "tsc",
    "compile:watch": "tsc --watch --preserveWatchOutput",
    "clean": "rm -rf build dist coverage tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dayjs": "^1.11.7",
    "defs": "workspace:*",
    "exif-parser": "^0.1.12",
    "ffmpeg-ffprobe-static": "^6.1.1",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.2.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "utils": "workspace:*",
    "node-utils": "workspace:*",
    "colorthief": "^2.6.0",
    "storage": "workspace:*"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.17.5",
    "@types/node": "^22.9.1",
    "typescript": "^5.6.3"
  }
}
