{
  "name": "backup",
  "description": "A backup tool for Photosphere.",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "s": "bun run start",
    "sd": "`bun run` start:dev",
    "start": "bun src/index.ts",
    "start:dev": "bun --hot src/index.ts",
    "c": "bun run compile",
    "cw": "bun run compile:watch",
    "compile": "tsc",
    "compile:watch": "tsc --watch --preserveWatchOutput",
    "clean": "rm -rf build dist coverage tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dayjs": "^1.11.7",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "mongodb": "^4.7.0",
    "storage": "workspace:*"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.17.5",
    "@types/node": "^22.9.1",
    "typescript": "^5.6.3"
  }
}
