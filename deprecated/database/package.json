{
  "name": "database",
  "description": "A tool for updating the Photosphere database.",
  "main": "src/index.js",
  "scripts": {
    "s": "pnpm run start",
    "sd": "pnpm run start:dev",
    "start": "rm -rf ./storage-test && tsx src/index.ts",
    "start:dev": "tsx watch src/index.ts",
    "c": "pnpm run compile",
    "cw": "pnpm run compile:watch",
    "compile": "tsc",
    "compile:watch": "tsc --watch --preserveWatchOutput",
    "clean": "rm -rf build dist coverage tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "colorthief": "^2.6.0",
    "dayjs": "^1.11.7",
    "defs": "workspace:*",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "mongodb": "^4.7.0",
    "node-utils": "workspace:*",
    "storage": "workspace:*",
    "utils": "workspace:*"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.5",
    "@types/node": "^22.9.1",
    "tsx": "^4.19.3",
    "typescript": "^5.6.3"
  }
}
