{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "compile:watch": {
            "dependsOn": [
                "compile"
            ],
            "outputs": [
                "build/**"
            ],
            "persistent": true
        },
        "compile": {
            "dependsOn": [
                "^compile"
            ],
            "outputs": [
                "build/**"
            ]
        },
        "photosphere-backend#start:dev": {
            "dependsOn": [
                "^compile"
            ],
            "persistent": true
        },
        "photosphere-frontend#start": {
            "dependsOn": [
                "^compile"
            ],
            "persistent": true
        }
    }
}